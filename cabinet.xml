    <reguli>
<!--    o regula cu if a and b and c then-->
<!--    Daca X este de talie mare si de culoare neagra si de gen feminin atunci are trasatura fioros.-->
        <regula>
            <if>
                <nume>X</nume>
                <var>mare</var>
                <rel>este</rel>
                <what>talie</what>
                <var>negru</var>
                <rel>este</rel>
                <what>culoare</what>
                <var>feminin</var>
                <rel>este</rel>
                <what>gen</what>
            </if>
            <then>
                <nume>X</nume>
                <var>fioros</var>
                <what>trasatura</what>
            </then>
        </regula>
        <regula>
<!--            luna profitabila -->
            <if>
                <luna>X</luna>
                <var>efectuat</var>
                <rel>este</rel>
                <what>detartraj</what>
                <var>efectuat</var>
                <rel>este</rel>
                <what>vaccin</what>
                <var>efectuat</var>
                <rel>este</rel>
                <what>castrare</what>
                <var>efectuat</var>
                <rel>este</rel>
                <what>tuns</what>
                <var>efectuat</var>
                <rel>este</rel>
                <what>vitamine</what>
            </if>
            <then>
                <luna>X</luna>
                <var>da</var>
                <what>profit</what>
            </then>
        </regula>
        <regula>
<!--           regula cu AND:  tratament intre 300 si 600 de lei -->
            <if>
                <tratament>X</tratament>
                <pret_minim>300</pret_minim>
                <pret_maxim>600</pret_maxim>
            </if>
            <then>
                <tratament>Castrare</tratament>
            </then>

<!--           regula cu AND:  tratament intre 0 si 300 de lei -->
            <if>
                <tratament>X</tratament>
                <pret_minim>0</pret_minim>
                <pret_maxim>300</pret_maxim>
            </if>
            <then>
                <tratament>Vaccin</tratament>
                <tratament>Vitamine</tratament>
            </then>
        </regula>
        <regula>
<!--           regula cu AND:  animal in functie de talie si gen-->
            <if>
                <animal>X</animal>
                <talie>mare</talie>
                <gen>feminin</gen>
            </if>
            <then>
                <animal>
                    <nume>Panti</nume>
                    <varsta>2</varsta>
                </animal>
            </then>

            <if>
                <animal>X</animal>
                <talie>medie</talie>
                <gen>feminin</gen>
            </if>
            <then>
                <animal>
                    <nume>Condorita</nume>
                    <varsta>18</varsta>
                </animal>
            </then>

            <if>
                <animal>X</animal>
                <talie>mica</talie>
                <gen>masculin</gen>
            </if>
            <then>
                <animal>
                    <nume>Marcel</nume>
                    <varsta>10</varsta>
                </animal>
            </then>
        </regula>
